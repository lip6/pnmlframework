<project xmlns="http://maven.apache.org/DECORATION/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd"
	name="PNML Document Checker">

	<publishDate position="right" format="yyyy-MM-dd" />
	<version position="right" />

<!-- Forget about the skin, reflow has security issues and is not maintained.	

	<skin>
		<groupId>lt.velykis.maven.skins</groupId>
		<artifactId>reflow-maven-skin</artifactId>
		<version>1.1.1</version>
	</skin> -->
	
		<bannerRight>
		<name>LIP6</name>
		<src>images/logo-Lip6.png</src>
		<href>http://www.lip6.fr</href>
	</bannerRight>
	

	<custom>
		<reflowSkin>
			<smoothScroll>true</smoothScroll>
			<!-- bootswatch-readable -->
			<theme>bootswatch-flatly</theme>
			<highlightJs>true</highlightJs>
			<highlightJsTheme>github</highlightJsTheme>
			<brand>
				<name>PNML Checker</name>
				<href>http://pnml.lip6.fr/pnmlvalidation/index.html</href>
			</brand>
			<slogan>Free Tool to Check your PNML Documents against the ISO/IEC
				15909-2 Specifications</slogan>
			<titleTemplate>%2$s | %1$s</titleTemplate>
			<toc>top</toc>
			<tocTopMax>6</tocTopMax>
			<topNav>Download|Documentation</topNav>
			<bottomNav maxSpan="9">
				<column>Overview</column>
				<column>Download|Documentation</column>
				<column>Find out more</column>
				<column>Contribute|reports|modules</column>
			</bottomNav>
			<bottomDescription>
			<![CDATA[
				 <span class="color-highlight">PNML Check</span> is a simple command-line tool to check your <a href="http://www.pnml.org">Petri Net Markup Language</a> Documents against 
				the <a href="http://www.iso.org/iso/catalogue_detail.htm?csnumber=43538">ISO/IEC 15909-2</a> Specifications (metamodels and constraints).
				<script type="text/javascript">
  					var _gaq = _gaq || [];
  					_gaq.push(['_setAccount', 'UA-48245698-1']);
  					_gaq.push(['_trackPageview']);

  					(function() {
    					var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    					ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    					var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  					})();
  				</script> 
			 ]]>
			</bottomDescription>
			<skinAttribution>false</skinAttribution>
			<pages>
				<index project="pnmlvalidation-Releng">
					<titleTemplate>PNML Document Checker</titleTemplate>
					<shortTitle>Welcome</shortTitle>
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
					<markPageHeader>false</markPageHeader>
					<sections>
						<body />
						<body />
						<columns>2</columns>
						<body />
					</sections>
				</index>
				<changelog project="pnmlvalidation-Releng">
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
				 	<sections>
				 		<body />
				 	</sections>
				</changelog>
				<source-repository>
					<highlightJs>false</highlightJs>
				</source-repository>
				<issue-tracking>
					<highlightJs>false</highlightJs>
					<toc>false</toc>
				</issue-tracking>
				<license>
					<highlightJs>false</highlightJs>
					<toc>false</toc>
				</license>
				<project-info>
					<toc>false</toc>
				</project-info>
				<github-report>
					<toc>false</toc>
				</github-report>
				<dependencies>
					<tocTopMax>4</tocTopMax>
				</dependencies>
				<developer-info>
					<toc>sidebar</toc>
				</developer-info>
			</pages>
		</reflowSkin>
	</custom>

